{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "LoggerHelper": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Fatal"
      }
    }
  },
    "AllowedHosts": "*"
  }
